{
  "name": "tsnodebase",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "lint": "tslint -c tslint.json 'src/**/*.ts'",
    "test": "nodemon --watch ./lib ./spec/runjasmine.js",
    "reload": "reload -d ./spec/results report.html",
    "clean": "rm -rf lib",
    "compile": "tsc && node lib/index.js",
    "dev": "concurrently --kill-others \"tsc -w\" \"nodemon ./lib/index.js\"",
    "crunch": "concurrently --kill-others \"npm run dev\" \"npm run test\" \"npm run reload\""
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^7.0.18",
    "concurrently": "^3.4.0",
    "jasmine": "^2.6.0",
    "jasmine-pretty-html-reporter": "^0.2.5",
    "ts-node": "^3.0.4",
    "typescript": "^2.3.2"
  },
  "dependencies": {
    "@types/body-parser": "^1.16.4",
    "@types/express": "^4.0.36",
    "@types/jasmine": "^2.5.47",
    "@types/xml-parser": "^1.2.29",
    "body-parser": "^1.17.2",
    "express": "^4.15.3",
    "reload": "^2.0.0",
    "xml-parser": "^1.2.1"
  }
}
